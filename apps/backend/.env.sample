SESSION_SECRET_KEY="a-secret-key"
SYNC_DATABASE_URL="sqlite:///./app.db"
ASYNC_DATABASE_URL="sqlite+aiosqlite:///./app.db"
PYTHONDONTWRITEBYTECODE=1

# LLM configuration (use provider + LLM_API_KEY; OPENAI_API_KEY is not read)
# Providers: openai | anthropic | raycast (choose one and set the model accordingly)
LLM_PROVIDER="llama_index.llms.anthropic.Anthropic"
LLM_API_KEY="<anthropic_key>"  # or leave empty and set ANTHROPIC_API_KEY to backfill
LLM_BASE_URL="https://api.anthropic.com"  # optional; Anthropic default shown
LL_MODEL="claude-sonnet-4-5"  # Anthropic Sonnet 4.5
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=2000

# Examples:
# Anthropic via LlamaIndex
# LLM_PROVIDER="llama_index.llms.anthropic.Anthropic"
# LLM_API_KEY="<anthropic_key>"  # or set ANTHROPIC_API_KEY and leave LLM_API_KEY empty
# LL_MODEL="claude-sonnet-4-5"

# OpenAI-like via LlamaIndex
# LLM_PROVIDER="llama_index.llms.openai_like.OpenAILike"
# LLM_BASE_URL="<your-openai-compatible-endpoint>"
# LLM_API_KEY="<api_key>"

# Embeddings (leave *_API_KEY empty to reuse LLM_API_KEY if supported by the provider)
EMBEDDING_PROVIDER="anthropic"  # e.g., openai | anthropic | raycast (if supported)
EMBEDDING_API_KEY=""
EMBEDDING_BASE_URL=""
EMBEDDING_MODEL="claude-3-5-sonnet-20240620"  # e.g., openai: text-embedding-3-large

# Note: OPENAI_API_KEY / ANTHROPIC_API_KEY / RAYCAST_API_KEY are not read directly;
# set LLM_API_KEY instead. Vendor-specific keys may be mapped into LLM_API_KEY if set.
